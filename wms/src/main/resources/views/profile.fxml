<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<BorderPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            stylesheets="@/styles/application.css, @../styles/profile.css"
            fx:controller="bg.tuvarna.sit.wms.controllers.ProfileController" prefHeight="576.0" prefWidth="1024.0">
    <left>
        <VBox fx:id="sideMenu" styleClass="side-menu"/>
    </left>
    <center>
        <VBox styleClass="section-my-profile" spacing="10">
            <Label styleClass="heading" text="My profile"/>
            <GridPane fx:id="profileSection" styleClass="grid-profile" hgap="10" vgap="10"
                      xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
                <padding><Insets top="10" right="10" bottom="10" left="10"/></padding>
            </GridPane>
        </VBox>
    </center>
</BorderPane>
